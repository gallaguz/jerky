{
    "name": "jerky",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "lint": "eslint ./apps/**",
        "lint:fix": "eslint ./apps/** --fix",
        "start": "nx serve",
        "build": "nx build",
        "test": "nx test",
        "test:cov": "npx jest --coverage",
        "api:test": "env-cmd -e api-microservice-test,amqp-test,jwt-test jest apps/api-microservice/src/app/api/auth-api/api.auth.controller.spec.ts",
        "auth:generate": "env-cmd -e authorization-microservice-development prisma generate --schema apps/authorization-microservice/src/prisma/schema.prisma",
        "auth:migrate:dev": "cd apps/authorization-microservice/src/ && prisma migrate dev",
        "auth:api:test": "env-cmd -e api-microservice-test,amqp-test,jwt-test jest apps/api-microservice/src/app/api/auth-api/api.auth.controller.spec.ts --verbose --runInBand --forceExit",
        "auth:test": "env-cmd -e authorization-microservice-test,amqp_test,jwt_test jest apps/authorization-microservice/src/app/auth/auth-controller.spec.ts",
        "user:generate": "env-cmd -e user-microservice-development prisma generate --schema apps/user-microservice/src/prisma/schema.prisma",
        "user:migrate:dev": "cd apps/user-microservice/src/ && prisma migrate dev",
        "user:db:seed": "ts-node apps/user/src/prisma/seed/index.ts",
        "user:test": "env-cmd -e user-microservice-test,amqp-test,jwt-test jest apps/user-microservice/src/app/user/user-controller.spec.ts",
        "catalog:generate": "env-cmd -e catalog-microservice-development prisma generate --schema apps/catalog-microservice/src/prisma/schema.prisma",
        "catalog:migrate:dev": "cd apps/catalog-microservice/src/ && prisma migrate dev",
        "catalog:db:seed": "ts-node apps/catalog/src/prisma/seed/index.ts",
        "catalog:db:reset": "cd apps/catalog/src/ && prisma migrate reset --force",
        "catalog:test:category": "env-cmd -e catalog-microservice-test,amqp-test,jwt-test jest apps/catalog-microservice/src/app/category/catalog-category-controller.spec.ts",
        "catalog:test:ingredient": "env-cmd -e catalog-microservice-test,amqp-test,jwt-test jest apps/catalog-microservice/src/app/ingredient/catalog-ingredient-controller.spec.ts",
        "catalog:test:product": "env-cmd -e catalog-microservice-test,amqp-test,jwt-test jest apps/catalog-microservice/src/app/product/catalog-product-controller.spec.ts",
        "catalog:test:raw": "env-cmd -e catalog-microservice-test,amqp-test,jwt-test jest apps/catalog-microservice/src/app/raw/catalog-raw-controller.spec.ts",
        "catalog:test:raw-type": "env-cmd -e catalog-microservice-test,amqp-test,jwt-test jest apps/catalog-microservice/src/app/raw-type/catalog-raw-type-controller.spec.ts",
        "catalog:test:recipe": "env-cmd -e catalog-microservice-test,amqp-test,jwt-test jest apps/catalog-microservice/src/app/recipe/catalog-recipe-controller.spec.ts",
        "catalog:test:recipe-type": "env-cmd -e catalog-microservice-test,amqp-test,jwt-test jest apps/catalog-microservice/src/app/recipe-type/catalog-recipe-type-controller.spec.ts",
        "catalog:test": "concurrently \"npm run catalog:test:category\" \"npm run catalog:test:ingredient\" \"npm run catalog:test:product\" \"npm run catalog:test:raw\" \"npm run catalog:test:raw-type\" \"npm run catalog:test:recipe\" \"npm run catalog:test:recipe-type\"",
        "prisma:seed": "concurrently \"npm run catalog:db:reset\" \"npm run catalog:db:seed\"",
        "prisma:generate": "concurrently \"npm run auth:generate\" \"npm run user:generate\" \"npm run catalog:generate\"",
        "prisma:migrate:dev": "concurrently \"npm run auth:migrate:dev\" \"npm run user:migrate:dev\" \"npm run catalog:migrate:dev\""
    },
    "private": false,
    "dependencies": {
        "@nestjs/common": "^9.0.0",
        "@nestjs/config": "2.2.0",
        "@nestjs/core": "^9.0.0",
        "@nestjs/jwt": "^10.0.1",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.0.0",
        "@prisma/client": "^4.8.1",
        "bcryptjs": "^2.4.3",
        "class-transformer": "^0.5.1",
        "class-validator": "0.14.0",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.0.3",
        "env-cmd": "^10.1.0",
        "es6-shim": "^0.35.6",
        "jsonwebtoken": "^9.0.0",
        "nest-winston": "^1.8.0",
        "nestjs-rmq": "2.8.0",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.0.0",
        "slug": "^8.2.2",
        "tslib": "^2.3.0",
        "uuidv4": "^6.2.13",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.1.2",
        "@commitlint/config-conventional": "^17.1.0",
        "@faker-js/faker": "^7.6.0",
        "@nestjs/schematics": "^9.0.0",
        "@nestjs/testing": "^9.0.0",
        "@nrwl/cli": "15.0.1",
        "@nrwl/cypress": "15.0.4",
        "@nrwl/eslint-plugin-nx": "15.0.1",
        "@nrwl/jest": "15.0.13",
        "@nrwl/linter": "15.0.1",
        "@nrwl/nest": "^15.0.13",
        "@nrwl/node": "15.0.13",
        "@nrwl/nx-cloud": "latest",
        "@nrwl/workspace": "15.0.1",
        "@types/bcryptjs": "^2.4.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/jest": "28.1.1",
        "@types/node": "18.7.1",
        "@types/passport-jwt": "^3.0.7",
        "@types/slug": "^5.0.3",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.41.0",
        "@typescript-eslint/parser": "^5.36.1",
        "concurrently": "^7.6.0",
        "cypress": "^10.7.0",
        "eslint": "8.22.0",
        "eslint-config-prettier": "8.1.0",
        "eslint-plugin-cypress": "^2.10.3",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.1",
        "jest": "28.1.1",
        "jest-environment-jsdom": "28.1.1",
        "nx": "15.0.1",
        "prettier": "^2.6.2",
        "prettier-plugin-prisma": "^4.4.0",
        "prisma": "^4.8.1",
        "supertest": "^6.3.1",
        "ts-jest": "28.0.5",
        "ts-node": "10.9.1",
        "typescript": "~4.8.2"
    }
}
